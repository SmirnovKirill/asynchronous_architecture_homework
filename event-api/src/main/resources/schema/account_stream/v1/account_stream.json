      "type" : [ "object", "null" ],
      "properties" : {
        "accountPublicId" : {
          "type" : [ "string", "null" ]
        },
        "balance" : {
          "type" : [ "number", "null" ]
        },
        "creationTime" : {
          "type" : [ "string", "null" ]
        },
        "eventType" : {
          "anyOf" : [ {
            "type" : "null"
          }, {
            "type" : "string",
            "enum" : [ "CREATE", "UPDATE", "DELETE" ]
          } ]
        },
        "userPublicId" : {
          "type" : [ "string", "null" ]
        }
      }
    },
    "eventId" : {
      "type" : [ "string", "null" ]
    },
    "eventName" : {
      "anyOf" : [ {
        "type" : "null"
      }, {
        "type" : "string",
        "enum" : [ "USER_STREAM", "USER_CREATED", "TASK_ASSIGNED", "TASK_STREAM", "TASK_CREATED", "TASK_RESOLVED", "ACCOUNT_STREAM", "ACCOUNT_BALANCE_CHANGED" ]
      } ]
    },
    "eventTime" : {
      "type" : [ "string", "null" ]
    },
    "eventVersion" : {
      "type" : "integer"
    },
    "producer" : {
      "type" : [ "string", "null" ]
    }
